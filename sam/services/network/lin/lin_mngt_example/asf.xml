<asf xmlversion="1.0">
	<module type="application" id="sam.services.lin.mngt_example" caption="LIN Example">
		<info type="description" value="summary">
		Basic example supporting Master and Slave Modes, using the LIN Software Stack. Requires the two SAM3X-EK boards and two USART-DB boards.
		</info>
		<info type="keyword" value="technology">
			<keyword value="Communication"/>
		</info>
		<build type="c-source" value="lin_mngt_example.c"/>
		<device-support value="sam3x"/>
		<require idref="sam.drivers.pio"/>
		<require idref="sam.drivers.pdc"/>
		<require idref="sam.drivers.pmc"/>
		<require idref="sam.drivers.tc"/>
		<require idref="sam.drivers.uart"/>
		<require idref="sam.drivers.usart"/>
		<require idref="sam.services.network.lin"/>
		<require idref="common.services.basic.gpio"/>
		<require idref="common.services.basic.clock"/>
		<require idref="common.utils.stdio.stdio_serial"/>
		<info type="gui-flag" value="move-to-root"/>
	</module>
</asf>
