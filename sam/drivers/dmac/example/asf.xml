<asf xmlversion="1.0">
	<module type="application" id="sam.drivers.dmac.example" caption="DMAC Example">
		<info type="description" value="summary">
		This example demonstrates user how to configurate and use DMAC controller for single and multiple buffer transfer.
		</info>
		<info type="keyword" value="technology">
			<keyword value="DMA Controller"/>
		</info>
		<build type="c-source" value="dmac_example.c"/>
		<build type="include-path" value="."/>
		<device-support value="sam3u"/>
		<device-support value="sam3x"/>
		<device-support value="sam4e"/>
		<require idref="sam.drivers.dmac"/>
		<require idref="sam.drivers.pmc"/>
		<require idref="sam.drivers.pio"/>
		<require idref="sam.drivers.uart"/>
		<require idref="common.services.basic.clock"/>
		<require idref="common.utils.stdio.stdio_serial"/>
		<info type="gui-flag" value="move-to-root"/>
	</module>
</asf>
