<asf xmlversion="1.0">
	<module type="application" id="sam.drivers.ast.example1" caption="AST Example 1">
		<info type="description" value="summary">
		Initialize the AST module, use with the 32KHz oscillator and use the USART to display a real time counter incremented every second.
		</info>
		<info type="keyword" value="technology">
			<keyword value="Timers"/>
		</info>
		<build type="c-source" value="ast_example1.c"/>
		<device-support value="sam4l"/>
		<require idref="sam.drivers.ast"/>
		<require idref="common.services.ioport"/>
		<require idref="common.services.basic.clock"/>
		<require idref="common.services.delay"/>
		<require idref="common.utils.stdio.stdio_serial"/>
		<info type="gui-flag" value="move-to-root"/>
	</module>
</asf>
