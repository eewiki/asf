<asf xmlversion="1.0">
	<project caption="Backup Power Manager Example on SAM4L-EK" id="sam.drivers.bpm.bpm_example.sam4l_ek">
		<info type="description" value="summary">
		This example shows how to configure BPM to switch power modes.
		</info>
		<build type="include-path" value="."/>
		<build type="header-file" value="conf_board.h"/>
		<build type="header-file" value="conf_clock.h"/>
		<build type="header-file" value="conf_uart_serial.h"/>
		<build type="header-file" value="conf_sleepmgr.h"/>
		<build type="header-file" value="conf_ast.h"/>
		<build type="include-path" value="."/>
		<require idref="sam.drivers.bpm.bpm_example"/>
		<require idref="board.sam4l_ek"/>
		<config name="config.board.sam4l_ek.bm" value="yes"/>
		<generator value="iarewarm"/>
		<generator value="armgcc"/>
		<generator value="as5_arm"/>
		<generator value="doxygen"/>
		<config name="config.compiler.iarewarm.create_aux" value="no"/>
		<config name="config.compiler.armgcc.create_aux" value="no"/>
		<config name="config.compiler.as5_arm.create_aux" value="no"/>
		<device-support value="atsam4lc4c"/>
	</project>
</asf>
