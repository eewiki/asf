<asf xmlversion="1.0">
	<select-by-device id="common.components.memory.eeprom.at24cxx.example">
		<module type="application" id="common.components.memory.eeprom.at24cxx.example#sam" caption="AT24CXX EEPROM Example">
			<info type="keyword" value="technology">
				<keyword value="Memory"/>
			</info>
			<info type="description" value="summary">
			Provide an application example using the Atmel EEPROM AT24C component. The EEPROM requires an I2C bus as a low level communication interface. This interface can be implemented using TWI controller for SAM devices.
			</info>
			<build type="c-source" value="at24cxx_example.c"/>
			<device-support value="sam"/>
			<require idref="common.services.basic.clock"/>
			<require idref="common.components.memory.eeprom.at24cxx"/>
			<require idref="common.utils.stdio.stdio_serial"/>
			<info type="gui-flag" value="move-to-root"/>
		</module>
	</select-by-device>
</asf>
