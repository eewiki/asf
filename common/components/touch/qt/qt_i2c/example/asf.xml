			<info type="description" value="summary">
				The application demonstrates how to configure a QTouch device and get device status.
			</info>
			<info type="keyword" value="technology">
				<keyword value="Touch"/>
			</info>
			<build type="include-path" value="."/>
			<build type="module-config" subtype="required-header-file" value="conf_example.h"/>
			<build type="c-source" value="example.c"/>
			<require idref="common.components.touch.qt.qt_i2c"/>
			<require idref="common.services.basic.clock"/>
			<require idref="common.utils.stdio.stdio_serial"/>
			<device-support value="sam4e"/>
			<info type="gui-flag" value="move-to-root"/>
		</module>
	</select-by-device>
</asf>
