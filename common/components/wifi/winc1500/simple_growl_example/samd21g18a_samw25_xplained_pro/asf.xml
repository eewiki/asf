<asf xmlversion="1.0">
	<project caption="WINC1500 Simple Growl Example for SAMW25 XPlained Pro" id="common.components.wifi.winc1500.simple_growl_example.samw25_xplained_pro">
		<!-- Summary Description -->
		<info type="description" value="summary">
			This example demonstrates the use of the WINC1500 with the SAMW25 Xplained Pro. It basically transmits a notification from the WINC1500 Wi-Fi module (based on a certain trigger) to a public remote server which in turn sends back a notification to a specific phone application.
		</info>

		<!-- Source -->
		<build type="include-path" value="."/>
		<build type="header-file" value="conf_board.h"/>
		<build type="header-file" value="conf_clocks.h"/>
		<build type="header-file" value="conf_extint.h"/>
		<build type="header-file" value="conf_spi.h"/>
		<build type="header-file" value="conf_winc.h"/>

		<!-- Device support -->
		<device-support value="atsamd21g18a"/>

		<!-- Required modules -->
		<require idref="common.components.wifi.winc1500.simple_growl_example"/>
		<require idref="board.samw25_xplained_pro"/>

		<!-- Generators -->
		<generator value="armgcc"/>
		<generator value="as5_arm"/>
		<generator value="doxygen"/>
		<config name="config.compiler.armgcc.create_aux" value="no"/>
		<config name="config.compiler.as5_arm.create_aux" value="no"/>
	</project>
</asf>
