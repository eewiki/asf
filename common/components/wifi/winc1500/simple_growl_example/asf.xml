<asf xmlversion="1.0">
	<select-by-device id="common.components.wifi.winc1500.simple_growl_example">
		<module type="application" id="common.components.wifi.winc1500.simple_growl_example#0" caption="WINC1500 Simple Growl Example">
			<!-- Summary Description -->
			<info type="description" value="summary">
				This example demonstrates the use of the WINC1500 with the SAMD21 Xplained Pro. It basically transmits a notification from the WINC1500 Wi-Fi module (based on a certain trigger) to a public remote server which in turn sends back a notification to a specific phone application.
			</info>

			<!-- Source -->
			<build type="include-path" value="."/>
			<build type="c-source" value="main_samd21.c"/>
			<build type="header-file" value="main.h"/>
			<build type="include-path" value="growl"/>
			<build type="c-source" value="growl/growl.c"/>
			<build type="header-file" value="growl/growl.h"/>

			<!-- Device support -->
			<device-support value="samd21"/>

			<!-- Required modules -->
			<require idref="common.components.wifi.winc1500"/>
			<require idref="sam0.utils"/>
			<require idref="sam0.utils.stdio"/>
			<require idref="sam0.utils.stdio.stdio_serial"/>
			<info type="gui-flag" value="move-to-root"/>
		</module>
		<module type="application" id="common.components.wifi.winc1500.simple_growl_example#1" caption="WINC1500 Simple Growl Example">
			<!-- Summary Description -->
			<info type="description" value="summary">
				This example demonstrates the use of the WINC1500 with the SAM4S Xplained Pro. It basically transmits a notification from the WINC1500 Wi-Fi module (based on a certain trigger) to a public remote server which in turn sends back a notification to a specific phone application.
			</info>

			<!-- Source -->
			<build type="include-path" value="."/>
			<build type="c-source" value="main.c"/>
			<build type="header-file" value="main.h"/>
			<build type="include-path" value="growl"/>
			<build type="c-source" value="growl/growl.c"/>
			<build type="header-file" value="growl/growl.h"/>

			<!-- Device support -->
			<device-support value="sam4s"/>

			<!-- Required modules -->
			<require idref="common.components.wifi.winc1500"/>
			<require idref="common.services.basic.clock"/>
			<require idref="common.utils.stdio.stdio_serial"/>
			<info type="gui-flag" value="move-to-root"/>
		</module>
		<module type="application" id="common.components.wifi.winc1500.simple_growl_example#2" caption="WINC1500 Simple Growl Example">
			<!-- Summary Description -->
			<info type="description" value="summary">
				This example demonstrates the use of the WINC1500 with the SAMG53 Xplained Pro. It basically transmits a notification from the WINC1500 Wi-Fi module (based on a certain trigger) to a public remote server which in turn sends back a notification to a specific phone application.
