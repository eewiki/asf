<asf xmlversion="1.0">
	<module type="application" id="common.services.calendar.example2" caption="Calendar Example 2">
		<device-support value="xmega"/>
		<device-support value="uc3"/>
		<info type="description" value="summary">
		An example application that calculates the time difference between two dates,
		and lights up a led if the resulting time is correct.
		</info>
		<build type="include-path" value="."/>
		<build type="c-source" value="example2.c"/>
		<require idref="common.services.basic.clock"/>
		<require idref="common.boards"/>
		<require idref="common.services.calendar"/>
		<info type="gui-flag" value="move-to-root"/>
	</module>
</asf>
