<asf xmlversion="1.0">
	<select-by-device id="common.services.usb.class.cdc.device.example3" caption="USB Device 7xCDC Example">
		<module type="application" id="common.services.usb.class.cdc.device.example3#1" caption="USB Device 7xCDC Example">
			<info type="description" value="summary">
			Provide an application example of USB Communication Device Class (CDC) implementation with 7 CDC interfaces.
			</info>
			<build type="header-file" value="conf_usb.h"/>
			<build type="header-file" value="main.h"/>
			<build type="header-file" value="ui.h"/>
			<build type="c-source" value="main.c"/>
			<build type="include-path" value="."/>
			<require idref="common.services.basic.clock"/>
			<require idref="common.services.basic.sleepmgr"/>
			<require idref="common.services.usb.class.device"/>
			<info type="keyword" value="technology">
				<keyword value="USB"/>
				<keyword value="Communication"/>
			</info>
			<device-support value="xmegaa1u"/>
			<device-support value="xmegaa3u"/>
			<device-support value="xmegaa3bu"/>
			<device-support value="xmegaa4u"/>
			<device-support value="xmegab"/>
			<device-support value="xmegac"/>
			<info type="gui-flag" value="move-to-root"/>
		</module>
	</select-by-device>
</asf>
