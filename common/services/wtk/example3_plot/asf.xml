<asf xmlversion="1.0">
	<module type="application" id="common.services.wtk.example3" caption="WTK (Widget ToolKit) Library Example 3">
		<info type="description" value="summary">
		Graphical widget library service example, demonstrating the plot widget, allowing
		the user to add new plot points to be graphed using a pair of slider and button widgets.
		</info>
		<info type="keyword" value="technology">
			<keyword value="Graphics"/>
			<keyword value="Touch"/>
		</info>
		<build type="c-source" value="example3.c"/>
		<build type="c-source" value="widget_gui.c"/>
		<build type="header-file" value="widget_gui.h"/>
		<build type="header-file" value="conf_membag.h"/>
		<build type="header-file" value="conf_sysfont.h"/>
		<build type="header-file" value="conf_wtk.h"/>
		<build type="header-file" value="conf_win.h"/>
		<build type="header-file" value="atmel_logo_small.h"/>
		<build type="include-path" value="."/>
		<device-support value="xmega"/>
		<device-support value="uc3"/>
		<device-support value="sam"/>
		<require idref="common.boards"/>
		<require idref="common.services.wtk"/>
		<require idref="common.components.touch.mxt"/>
		<require idref="common.services.basic.clock"/>
		<info type="gui-flag" value="move-to-root"/>
	</module>
</asf>
