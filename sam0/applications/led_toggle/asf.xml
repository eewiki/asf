<asf xmlversion="1.0">
	<module type="application" id="sam0.applications.led_toggle" caption="LED Toggle Application">
		<info type="description" value="summary">
		Example application to toggle the board LED when a button is pressed, using various device drivers.
		</info>

		<info type="keyword" value="technology">
			<keyword value="Interrupt" />
			<keyword value="I/O" />
			<keyword value="CPU" />
		</info>

		<info type="gui-flag" value="move-to-root"/>

		<build type="c-source" value="led_toggle.c"/>

		<build type="include-path" value="."/>

		<require idref="sam0.drivers.port"/>
		<require idref="sam0.drivers.extint"/>
		<require idref="sam0.drivers.system"/>

		<device-support value="samd20"/>
		<device-support value="samd21"/>
	</module>
</asf>
