<asf xmlversion="1.0">
	<module type="application" id="avr32.services.dsplib.example.dsp32_signal_generation" caption="DSPLIB - 32-bit fixed-point signal generation example">
		<info type="description" value="summary">
		Example application. It demonstrates how to generate 32-bit fixed-point signals using the function set provided in the DSP library.
		</info>
		<info type="keyword" value="technology">
			<keyword value="Digital Signal Processing"/>
		</info>
		<build type="c-source" value="signal_generation_example.c"/>
		<device-support value="uc3"/>
		<require idref="avr32.drivers.pm"/>
		<require idref="avr32.drivers.cpu.cycle_counter"/>
		<require idref="avr32.services.dsplib.signal_generation"/>
		<info type="gui-flag" value="move-to-root"/>
	</module>
</asf>
