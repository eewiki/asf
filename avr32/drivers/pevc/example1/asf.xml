<asf xmlversion="1.0">
	<module type="application" id="avr32.drivers.pevc.example1" caption="PEVC Example 1">
		<info type="description" value="summary">
		PEVC example using the "AST - period event 0" as an event source and the "PDCA - start of transfer channel 0" as a triggered event that sends a character to a USART output.
		</info>
		<info type="keyword" value="technology">
			<keyword value="Peripheral Event System"/>
		</info>
		<build type="c-source" value="pevc_example1.c"/>
		<build type="header-file" value="conf_clock.h"/>
		<build type="include-path" value="."/>
		<device-support value="uc3c"/>
		<require idref="avr32.drivers.ast"/>
		<require idref="avr32.drivers.cpu.cycle_counter"/>
		<require idref="avr32.drivers.gpio"/>
		<require idref="avr32.drivers.intc"/>
		<require idref="avr32.drivers.pdca"/>
		<require idref="avr32.drivers.pevc"/>
		<require idref="avr32.drivers.pm"/>
		<require idref="common.services.delay"/>
		<require idref="avr32.utils.print_funcs"/>
		<info type="gui-flag" value="move-to-root"/>
	</module>
</asf>
