<asf xmlversion="1.0">
	<project caption="XMEGA-A1 Xplained with Xplained Light/Proximity Board #1" id="xmega.applications.xmega-a1_avrsblp1_visualizer">
		<info type="description" value="summary">
			Demo application for XMEGA-A1 Xplained obtains sensor data from the
			light and proximity sensor device on an Atmel Sensors Xplained
			Light/Proximity 1 board (ATAVRSBLP1).  The data is
			then formatted and transferred to a remote host for display using
			the Atmel Data Visualizer tool.
		</info>
		<config name="config.common.services.basic.twi.mode" value="master"/>
		<config name="config.board.xmega_a1_xplained.led" value="yes"/>
		<build type="header-file" value="conf/conf_board.h"/>
		<build type="header-file" value="conf/conf_twim.h"/>
		<build type="header-file" value="conf/conf_usart_serial.h"/>
		<build type="include-path" value="./conf"/>
		<require idref="board.xmega_a1_xplained"/>
		<require idref="board.sensors_xplained.lightprox_1"/>
		<require idref="common.utils.stdio.stdio_serial"/>
		<require idref="sensors.lightprox.visualizer"/>
		<build type="define" name="CONF_STDIO_REDIRECT" value=""/>
		<generator value="iarew"/>
		<generator value="avrgcc"/>
		<generator value="as5_8"/>
		<generator value="doxygen"/>
		<device-support value="atxmega128a1"/>
	</project>
