		</info>
		<!-- Keyword for sorting in example project wizard -->
		<info type="keyword" value="technology">
			<keyword value="Timers"/>
		</info>
		<!-- Device support -->
		<device-support value="xmega"/>
		<!-- Include paths -->
		<build type="include-path" value="."/>
		<build type="module-config" subtype="required-header-file" value="conf_example.h"/>
		<!-- Lists build information and dependencies -->
		<build type="c-source" value="example2.c"/>
		<require idref="common.services.basic.gpio"/>
		<require idref="common.services.basic.clock"/>
		<require idref="xmega.drivers.pmic"/>
		<require idref="xmega.services.timeout"/>
		<require idref="common.services.basic.gpio"/>
		<info type="gui-flag" value="move-to-root"/>
	</module>
</asf>
