	<select-by-config id="config.thirdparty.lwip.141.ipv" name="config.thirdparty.lwip.141.ipv" caption="IPv4 versus IPv6 configuration of lwIP v141" default="4">
		<module type="meta" id="config.thirdparty.lwip.141.ipv#4" caption="IPv4 support in lwIP v141">
