<asf xmlversion="1.0">
	<project force-caption="true" force-description="true" caption="Single button controller application ATmegarfr2-ATmega256RFR2 Xplained Pro" id="thirdparty.wireless.avr2102_rf4control.apps.zrc.single_button_ctr.ncp.atmega256rfr2_xplained_pro">
		<info type="description" value="summary">
		The RF4CE-ZRC single button controller application where the application and the stack running on the same node
		</info>
		<build type="include-path" value="."/>
		<build type="header-file" value="conf_board.h"/>
		<build type="header-file" value="conf_clock.h"/>
		<build type="header-file" value="conf_sio2host.h"/>
		<build type="header-file" value="conf_usart_serial.h"/>
        <build type="header-file" value="conf_nvm.h"/>
		<build type="header-file" value="conf_flash.h"/>
		<build type="header-file" value="conf_hw_timer.h"/>
